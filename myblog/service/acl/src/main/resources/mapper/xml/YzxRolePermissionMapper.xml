<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yzx.acl.mapper.YzxRolePermissionMapper">

    <delete id="removeByPermissionIds">
      delete from `yzx_role_permission` where permission_id in
      <foreach collection="ids" item="id" separator="," open="(" close=")">
          #{id}
      </foreach>
      and role_id=#{roleId}
    </delete>
</mapper>
